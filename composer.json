{
  "description": "Defalto CRM",
  "type": "project",
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-exif": "*",
    "ext-fileinfo": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-imap": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "ext-zip": "*",
    "adodb/adodb-php": "5.*",
    "apalfrey/select2-bootstrap-5-theme": "*",
    "ckeditor/ckeditor": "4.22.*",
    "components/font-awesome": "*",
    "ezyang/htmlpurifier": "4.*",
    "fullcalendar/fullcalendar": "*",
    "google/apiclient": "2.*",
    "kcfinder/kcfinder": "*",
    "league/oauth2-client": "^2.7",
    "league/oauth2-google": "^4.0",
    "michelf/php-markdown": "1.*",
    "monolog/monolog": "^3.5",
    "mpdf/mpdf": "*",
    "mpdf/qrcode": "*",
    "myclabs/deep-copy": "^1.7",
    "paragonie/random_compat": "^1.4|^2.0|^9.99.99",
    "php-http/message-factory": "^1.0",
    "phpmailer/phpmailer": "*",
    "phpoffice/phpspreadsheet": "*",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0 || ^2.0",
    "select2/select2": "*",
    "setasign/fpdi": "^2.1",
    "simplehtmldom/simplehtmldom": "*",
    "smarty/smarty": "4.*",
    "twbs/bootstrap": "*",
    "twbs/bootstrap-icons": "*",
    "webklex/php-imap": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "1.9.*",
    "rector/rector": "0.15.*",
    "tracy/tracy": "2.*"
  },
  "scripts": {
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  },
  "extra": {
    "google/apiclient-services": [
      "Calendar",
      "Oauth2",
      "PeopleService"
    ]
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "fullcalendar/fullcalendar",
        "version": "5.11.3",
        "dist": {
          "url": "https://github.com/fullcalendar/fullcalendar/releases/download/v5.11.3/fullcalendar-5.11.3.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "kcfinder/kcfinder",
        "source": {
          "url": "https://github.com/IT-Solutions4You/kcfinder.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "classmap": ["/"]
        },
        "version": "1"
      }
    },
    {
      "type": "package",
      "package": {
        "name": "mpdf/mpdf",
        "source": {
          "url": "https://github.com/IT-Solutions4You/mpdf.git",
          "type": "git",
          "reference": "development"
        },
        "autoload": {
          "classmap": ["/src"]
        },
        "version": "1"
      }
    },
    {
      "type": "package",
      "package": {
        "name": "simplehtmldom/simplehtmldom",
        "source": {
          "url": "https://github.com/simplehtmldom/simplehtmldom.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "classmap": ["/"]
        },
        "version": "1"
      }
    }
  ],
  "config": {
    "bin-dir": "bin",
    "discard-changes": true,
    "optimize-autoloader": true
  },
  "authors": [
    {
      "name": "Defalto",
      "email": "info@its4you.sk"
    }
  ],
  "autoload": {
    "files": ["includes/Loader.php"]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}